{
  "manifest_version": 2,

  "name": "Hedgehog",
  "description": "Analyses recent Instagram posts and create a word cloud describing the user.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://api.projectoxford.ai/",
    "https://api.instagram.com/"
  ]
}